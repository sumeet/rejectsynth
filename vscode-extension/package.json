{
  "name": "vscode-rejectsynth",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo OK"
  },
  "author": "",
  "license": "ISC",
  "engines": {
    "vscode": "^1.83.0"
  },
  "activationEvents": [
    "onLanguage:rejectsynth"
  ],
  "configurationDefaults": {
    "rejectsynth": {
      "editor.semanticHighlighting.enabled": true
    }
  },
  "contributes": {
    "commands": [
      {
        "command": "rejectsynth.playFromHere",
        "title": "Play from Here"
      },
      {
        "command": "rejectsynth.stopPlaying",
        "title": "Stop Playing"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "rejectsynth.playFromHere",
          "when": "editorTextFocus"
        }
      ]
    },
    "configuration": {
      "properties": {}
    },
    "languages": [
      {
        "id": "rejectsynth",
        "aliases": [
          "rejectsynth"
        ],
        "extensions": [
          ".rej"
        ]
      }
    ]
  },
  "dependencies": {
    "naudiodon": "^2.3.6",
    "speaker": "^0.5.4"
  },
  "devDependencies": {
    "@electron/rebuild": "^3.3.0"
  }
}
